        <!-- Spinner End -->
        <!-- Header Start -->
        @await Component.InvokeAsync("_NavbarPartial")
        <!-- Header End -->
        <!-- Carousel Start -->
        @await Component.InvokeAsync("_SliderPartial")
        <!-- Carousel End -->
        <!-- Booking Start -->
      @*  @await Component.InvokeAsync("_ReservationPartial")*@
        <!-- Booking End -->
        <!-- About Start -->
        @await Component.InvokeAsync("_AboutPartial")
        <!-- About End -->
        <!-- Room Start -->
        @await Component.InvokeAsync("_RoomPartial")
       
        <!-- Room End -->
        <!-- Video Start -->
        @await Component.InvokeAsync("_TrailerPartial")
   
        <!-- Video Start -->
        <!-- Service Start -->
        @await Component.InvokeAsync("_ServicePartial")
      
        <!-- Service End -->
        <!-- Testimonial Start -->
        @await Component.InvokeAsync("_TestimonialPartial")
        
        <!-- Testimonial End -->
        <!-- Team Start -->
        @await Component.InvokeAsync("_TeamPartial")
        <!-- Team End -->
        <!-- Newsletter Start -->
        @await Html.PartialAsync("_SubscribePartial.cshtml")
      
        <!-- Newsletter Start -->
        <!-- Footer Start -->
        @await Component.InvokeAsync("_FooterPartial")
        <!-- Footer End -->
        <!-- Back to Top -->
        @await Component.InvokeAsync("_ScriptPartial")
    

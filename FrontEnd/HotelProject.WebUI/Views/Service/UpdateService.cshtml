@model UpdateServiceDto
@{
    ViewData["Title"] = "UpdateService";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}
<form class="form-valide" method="post">
    <input type="hidden" asp-for="ID" />
    <div class="form-group row">
        <label class="col-lg-4 col-form-label">Hizmet İkonu </label>
        <div class="col-lg-6">
            <input type="text" class="form-control" asp-for="ServiceIcon" />
        </div>
    </div>
    <div class="form-group row">
        <label class="col-lg-4 col-form-label">Personel Ünvanı </label>
        <div class="col-lg-6">
            <input type="text" class="form-control" asp-for="Title" />
        </div>
    </div>
    <div class="form-group row">
        <label class="col-lg-4 col-form-label">Açıklama </label>
        <div class="col-lg-6">
            <textarea type="text" class="form-control" asp-for="Description" ></textarea>
        </div>
    </div>
   
    <button class="btn mb-1 btn-secondary">
        Güncelle <span class="btn-icon-right"><i class="fa fa-upload"></i></span>
    </button>

</form>


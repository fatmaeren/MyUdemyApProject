
@{
    Layout = null;
}

<!DOCTYPE html>

<!DOCTYPE html>
<html lang="en">

@await Html.PartialAsync("/Views/AdminLayout/PartialView.cshtml")

<body>

 
    <div id="preloader">
        <div class="loader">
            <svg class="circular" viewBox="25 25 50 50">
                <circle class="path" cx="50" cy="50" r="20" fill="none" stroke-width="3" stroke-miterlimit="10" />
            </svg>
        </div>
    </div>
  
    <div id="main-wrapper">

     
        <div class="nav-header">
            <div class="brand-logo">
                <a href="/Default/Index">
                    <b class="logo-abbr"><img src="images/logo.png" alt=""> </b>
                    <span class="logo-compact"><img src="~/adminweb/images/logo-compact.png" alt=""></span>
                    <span class="brand-title">
                        <img src="~/adminweb/images/logo-text.png" alt="">
                    </span>
                </a>
            </div>
        </div>
     
        @await Html.PartialAsync("/Views/AdminLayout/HeaderView.cshtml")
     
        @await Html.PartialAsync("/Views/AdminLayout/SidebarView.cshtml")
       
    
        <div class="content-body">

            <div class="row page-titles mx-0">
                <div class="col p-md-0">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="javascript:void(0)">Dashboard</a></li>
                        <li class="breadcrumb-item active"><a href="javascript:void(0)">Home</a></li>
                    </ol>
                </div>
            </div>
      

            <div class="container-fluid">
                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <div class="card-body">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                </div>
            </div>
          
        </div>
    
        @await Html.PartialAsync("/Views/AdminLayout/FooterView.cshtml")
     
       
    </div>
   
    <script src="~/adminweb/plugins/common/common.min.js"></script>
    <script src="~/adminweb/js/custom.min.js"></script>
    <script src="~/adminweb/js/settings.js"></script>
    <script src="~/adminweb/js/gleek.js"></script>
    <script src="~/adminweb/js/styleSwitcher.js"></script>

</body>

</html>